an>\n        </el-menu-item>\n\n        <!--有多个子元素的菜单资源多级显示-->\n        <el-submenu v-else :index=\"item.name\" :key=\"item.name\" popper-class=\"popper-item\">\n          <template slot=\"title\">\n            <i :class=\"[item.meta.icon || 'fa fa-navicon']\"></i>\n            <span slot=\"title\">{{ item.meta.title }}</span>\n          </template>\n\n          <template v-for=\"child in item.children\">\n            <template v-if=\"!child.hidden\">\n              <!--多级子菜单循环嵌套-->\n              <sidebar-item\n                v-if=\"child.children && child.children.length > 0\"\n                :is-nest=\"true\"\n                class=\"nest-menu\"\n                :menu=\"[child]\"\n                :key=\"child.name\"\n              >\n              </sidebar-item>\n\n              <el-menu-item v-else :index=\"child.name\" :key=\"child.name\">\n                <i :class=\"[child.meta.icon || 'fa fa-navicon']\"></i>\n                <span slot=\"title\">{{ child.meta.title }}</span>\n              </el-menu-item>\n            </template>\n          </template>\n        </el-submenu>\n      </template>\n    </template>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'SidebarItem',\n  props: {\n    menu: {\n      type: Array,\n    },\n    isNest: {\n      type: Boolean,\n      default: false,\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}